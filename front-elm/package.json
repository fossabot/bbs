{
  "license": "MIT",
  "devDependencies": {
    "elm": "^0.19.0",
    "elm-analyse": "^0.16.4",
    "elm-format": "^0.8.1",
    "elm-live": "^3.4.0",
    "elm-test": "^0.19.0",
    "uglify-js": "^3.5.15"
  },
  "scripts": {
    "watch": "npx elm-live src/Main.elm --pushstate=true -o -d static -- --output=static/main.js",
    "analyse": "npx elm-analyse -o -s",
    "test": "npx elm-test",
    "make": "npx elm make src/Main.elm --optimize --output static/main.js && npm run compress",
    "compress": "npx uglifyjs static/main.js --compress \"pure_funcs=[F2,F3,F4,F5,F6,F7,F8,F9,A2,A3,A4,A5,A6,A7,A8,A9],pure_getters,keep_fargs=false,unsafe_comps,unsafe\" | npx uglifyjs --mangle --output=static/main.js",
    "clean": "rm -rf static/main.js"
  },
  "dependencies": {}
}
